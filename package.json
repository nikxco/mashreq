{
  "name": "@mashreq/monorepo",
  "version": "1.0.0",
  "description": "Monorepo for mashreq assessment apps",
  "private": "true",
  "workspaces": [
    "apps/*",
    "services/*"
  ],
  "scripts": {
    "start:web": "npm --prefix ./apps/web run start",
    "build:web": "npm --prefix ./apps/web run build",
    "test:web": "npm --prefix ./apps/web run test",
    "start:api": "npm --prefix ./services/rest-api run start:dev",
    "build:api": "npm --prefix ./services/rest-api run build"
  },
  "author": "Nadeem Ahmad",
  "devDependencies": {
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "typescript": "^5.3.3"
  }
}